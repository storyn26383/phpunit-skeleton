{
    "name": "sasaya/phpunit-skeleton",
    "description": "PHPUnit Skeleton",
    "keywords": [
        "phpunit"
    ],
    "homepage": "https://github.com/storyn26383/phpunit-skeleton",
    "authors": [
        { "name": "Sasaya", "email": "sasaya@percussion.life" }
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.12",
        "laravel/pint": "^1.17",
        "robiningelbrecht/phpunit-pretty-print": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Sasaya\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sasaya\\Tests\\": "tests/"
        }
    },
    "scripts": {
      "test": "phpunit",
      "phpstan": "phpstan analyse",
      "cs": "pint src tests --test",
      "cs:fix": "pint src tests"
    }
}
